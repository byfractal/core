OPENAI_API_KEY=
# Test mode configuration
TESTING=false

# Model configuration
DEFAULT_MODEL=gpt-3.5-turbo

# Analytics provider configuration (posthog, mixpanel, amplitude)
ANALYTICS_PROVIDER=posthog

# PostHog configuration
# Using a Personal API Key (phx_*) which requires Bearer authentication
POSTHOG_API_KEY=
POSTHOG_PROJECT_ID=
POSTHOG_API_URL=
POSTHOG_FEEDBACK_EVENT=feedback_submitted

# Supabase configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Security Configuration
# Auth0 Configuration
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_API_AUDIENCE=https://api.yourdomain.com
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret
AUTH0_CALLBACK_URL=https://your-callback-url.com/callback

# Encryption
MASTER_KEY=your_master_encryption_key_at_least_32_characters
ENCRYPTION_KEYS_FILE=encryption_keys.json

# Rate Limiting
REDIS_URL=redis://localhost:6379/0
RATE_LIMIT_DEFAULT=100
RATE_LIMIT_WINDOW_SECONDS=60

# CORS Settings
CORS_ORIGINS=*

# Environment
ENVIRONMENT=development

# Application Configuration
DEBUG=true
HOST=0.0.0.0
PORT=8000
FRONTEND_URL=http://localhost:3000

# Security Configuration
USE_AUTH0=true

# JWT Authentication (if not using Auth0)
JWT_SECRET_KEY=your-jwt-secret-key

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/app_db
